<%= form_for @item, html: {multipart: true} do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>
    <%= f.submit 'Add item', class: "btn btn-primary"  %>
<% end %>
